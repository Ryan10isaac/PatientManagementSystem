<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hospitalmanagement">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.YiseulKo_FranzCadiente_COMP304Sec003_Lab05_Ex1">
        <activity
            android:name="com.example.hospitalmanagement.ViewTestInfoActivity"
            android:exported="false"
            android:parentActivityName="com.example.hospitalmanagement.TestListActivity"/>
        <activity
            android:name="com.example.hospitalmanagement.TestActivity"
            android:exported="false"
            android:parentActivityName="com.example.hospitalmanagement.TestListActivity" />
        <activity
            android:name="com.example.hospitalmanagement.TestListActivity"
            android:exported="false"
            android:launchMode="singleTop"
            android:parentActivityName="com.example.hospitalmanagement.MainActivity" />
        <activity
            android:name="com.example.hospitalmanagement.UpdateInfoActivity"
            android:exported="false"
            android:parentActivityName="com.example.hospitalmanagement.PatientListActivity" />
        <activity
            android:name="com.example.hospitalmanagement.PatientListActivity"
            android:exported="false"
            android:launchMode="singleTop"
            android:parentActivityName="com.example.hospitalmanagement.MainActivity" />
        <activity
            android:name="com.example.hospitalmanagement.PatientActivity"
            android:exported="false"
            android:parentActivityName="com.example.hospitalmanagement.PatientListActivity" />
        <activity
            android:name="com.example.hospitalmanagement.RegisterActivity"
            android:exported="false" />
        <activity
            android:name="com.example.hospitalmanagement.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.hospitalmanagement.MainActivity"
            android:exported="false"
            android:launchMode="singleTop" />
    </application>

</manifest>